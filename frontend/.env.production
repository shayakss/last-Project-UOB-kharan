# Production Environment Configuration

# Backend Configuration
REACT_APP_BACKEND_URL=__BACKEND_URL_PLACEHOLDER__

# Feature Flags
REACT_APP_ENABLE_DEV_TOOLS=false
REACT_APP_ENABLE_LOGS=false
REACT_APP_API_TIMEOUT=30000

# Production Settings
GENERATE_SOURCEMAP=true
FAST_REFRESH=false
BROWSER=none
CI=true

# Error Handling
REACT_APP_RETRY_ATTEMPTS=5
REACT_APP_RETRY_DELAY=2000

# Performance
REACT_APP_CHUNK_SIZE_WARNING_LIMIT=500000

# Security
REACT_APP_SECURE_MODE=true